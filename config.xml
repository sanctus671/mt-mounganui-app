<?xml version='1.0' encoding='utf-8'?>
<widget id="nz.net.mountmaunganui.mtbizdirectory" version="2.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" versionCode="201011">

  <name>Mt Biz Directory</name>
  <description>
        Find, discover and connect with local Mount Maunganui businesses, in our easy to use app. From restaurants to builders, the Mount Maunganui Biz Directory has it all.
  </description>
  <author email="Matti.TS.1995@gmail.com" href="http://www.thorley-symes.com">
    Matti Thorley-Symes
  </author>
  
  
    <content src="index.html" />
	<preference name="phonegap-version" value="cli-6.3.0" />
	<preference name="android-build-tool" value="gradle" /> 
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
	

    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="15" />
	<preference name="android-targetSdkVersion" value="23" />
    <preference name="android-installLocation" value="auto" />

	<preference name="SplashScreen" value="resources/splash.png" />
	<preference name="AutoHideSplashScreen" value="true" />
	<preference name="SplashMaintainAspectRatio" value="true" />
	<preference name="SplashShowOnlyFirstTime" value="true" />
	<preference name="SplashScreenBackgroundColor" value="white" />
	
	
    <gap:plugin name="cordova-plugin-network-information" source="npm"/>
    <gap:plugin name="cordova-plugin-splashscreen" source="npm"/>

	<gap:plugin name="cordova-plugin-statusbar" source="npm"/>	

	<gap:plugin name="cordova-plugin-compat" source="npm"/>	
	<gap:plugin name="cordova-plugin-console" source="npm"/>	
	<gap:plugin name="cordova-plugin-device" source="npm"/>	
	<gap:plugin name="cordova-plugin-geolocation" source="npm"/>	
	<gap:plugin name="cordova-plugin-transport-security" source="npm"/>	
	
	
	
	<gap:plugin name="cordova-plugin-email-composer" source="npm"/>	

	<gap:plugin name="ionic-plugin-keyboard" source="npm"/>	
	
	<gap:plugin name="phonegap-plugin-push" source="npm" version="1.7.0">
		<param name="SENDER_ID" value="345453645665" />
	</gap:plugin>	
	
	<gap:plugin name="cordova-plugin-inappbrowser" source="npm"/>
	
	<gap:plugin name="cordova-plugin-x-socialsharing" source="npm" />
	
	<gap:plugin name="cordova-plugin-whitelist" source="npm"/>	
	
	
	

    <icon gap:platform="android" gap:qualifier="ldpi" src="resources/android/icon/drawable-ldpi-icon.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="resources/android/icon/drawable-mdpi-icon.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="resources/android/icon/drawable-hdpi-icon.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="resources/android/icon/drawable-xhdpi-icon.png" />
	<icon gap:platform="android" gap:qualifier="xxhdpi" src="resources/android/icon/drawable-xxhdpi-icon.png" />
	<icon gap:platform="android" gap:qualifier="xxxhdpi" src="resources/android/icon/drawable-xxxhdpi-icon.png" />	
	
	<icon gap:platform="ios" src="resources/ios/icon/icon.png" width="57" height="57"/>
	<icon gap:platform="ios" src="resources/ios/icon/icon-2x.png" width="114" height="114"/>
	<icon gap:platform="ios" src="resources/ios/icon/icon-40.png" width="40" height="40"/>
